        <rdfs:subClassOf rdf:resource="&obo;BFO_0000002"/>
        <obo:IAO_0000115 xml:lang="en">An environment in which system access is not controlled by persons who are responsible for the content of electronic records that are on the system.</obo:IAO_0000115>
    </owl:Class>
    


